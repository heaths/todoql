{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/dist/graphql",
      "preLaunchTask": "serve",
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "http://localhost:([0-9]+)/graphql",
        "uriFormat": "http://localhost:%s/graphql/play"
      }
    }
  ]
}
